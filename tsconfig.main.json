{
	"compilerOptions": {
		"outDir": "./build/",
		"noImplicitAny": true,
		"module": "es6",
		"target": "es5",
		"allowJs": true,
		"baseUrl": ".",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"moduleResolution": "Node",
		"paths": {
			"*": ["node_modules/*"]
		}
	},
	"include": ["main", "src/model"]
}
